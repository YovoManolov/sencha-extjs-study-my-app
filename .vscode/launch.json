{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome with Ext JS",
            "type": "pwa-chrome",
            "request": "launch",
            "url": "http://localhost:42011/", 
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///*": "${webRoot}/*"
            }
        },
        {
            "name": "Attach to Chrome",
            "type": "pwa-chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        }
    ]
}
